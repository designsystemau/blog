{
  "name": "eleventy-duo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pancake": "node_modules/.bin/pancake",
    "dev": "run-p dev:*",
    "start": "eleventy --serve",
    "build": "run-s clean build:assets build:site",
    "dev:assets": "env ELEVENTY_ENV=development webpack --mode production --watch",
    "dev:site": "env ELEVENTY_ENV=development eleventy --serve",
    "build:assets": "webpack --mode production",
    "build:site": "env ELEVENTY_ENV=production eleventy",
    "clean": "rm -rf ./public"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-img": "^0.6.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eleventy-plugin-reading-time": "^0.0.1",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "remove": "^0.1.5",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "dependencies": {
    "@gov.au/accordion": "latest",
    "@gov.au/animate": "latest",
    "@gov.au/body": "latest",
    "@gov.au/breadcrumbs": "latest",
    "@gov.au/buttons": "latest",
    "@gov.au/callout": "latest",
    "@gov.au/card": "latest",
    "@gov.au/control-input": "latest",
    "@gov.au/core": "^4.0.0",
    "@gov.au/cta-link": "latest",
    "@gov.au/direction-links": "latest",
    "@gov.au/footer": "latest",
    "@gov.au/form": "latest",
    "@gov.au/grid-12": "latest",
    "@gov.au/header": "latest",
    "@gov.au/headings": "latest",
    "@gov.au/inpage-nav": "latest",
    "@gov.au/keyword-list": "latest",
    "@gov.au/link-list": "latest",
    "@gov.au/main-nav": "latest",
    "@gov.au/page-alerts": "latest",
    "@gov.au/progress-indicator": "latest",
    "@gov.au/responsive-media": "latest",
    "@gov.au/searchbox": "latest",
    "@gov.au/select": "latest",
    "@gov.au/side-nav": "latest",
    "@gov.au/skip-link": "latest",
    "@gov.au/table": "latest",
    "@gov.au/tags": "latest",
    "@gov.au/text-inputs": "latest",
    "copy-webpack-plugin": "^9.0.1"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [
      "@gov.au/pancake-react",
      "@gov.au/pancake-json"
    ],
    "css": {
      "minified": false,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "pancake/css/",
      "name": "designsystem.css"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "pancake/js/",
      "name": "designsystem.min.js"
    },
    "sass": {
      "modules": false,
      "location": "pancake/sass/",
      "name": "pancake.scss"
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie 8",
    "ie 9",
    "ie 10"
  ]
}